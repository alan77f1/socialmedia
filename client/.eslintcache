\r\n\r\n  return {\r\n    errMsg: err,\r\n    errLength: Object.keys(err).length,\r\n  };\r\n};\r\n\r\nfunction validateEmail(email) {\r\n  // eslint-disable-next-line\r\n  const re =\r\n    /^(([^<>()[\\]\\\\.,;:\\s@\\\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\\\"]+)*)|(\\\".+\\\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/;\r\n  return re.test(email);\r\n}\r\n\r\nexport default valid;\r\n","D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\alertReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\authReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\themeReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\statusReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\profileReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\modalReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\postReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\detailPostReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\discoverReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\socketReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\suggestionsReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\notifyReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\onlineReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\peerReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\callReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\messageReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\actions\\discoverAction.js",[],"D:\\TT\\utc2-network\\client\\src\\components\\message\\LeftSide.js",[],"D:\\TT\\utc2-network\\client\\src\\components\\message\\RightSide.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\actions\\profileAction.js",[],"D:\\TT\\utc2-network\\client\\src\\components\\profile\\Info.js",[],"D:\\TT\\utc