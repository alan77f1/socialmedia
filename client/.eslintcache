entAction.js",[],"D:\\UTC2_Social_Network\\client\\src\\components\\home\\comments\\CommentCard.js",[],"D:\\UTC2_Social_Network\\client\\src\\components\\home\\comments\\CommentMenu.js",[],["427","428"],"D:\\UTC2_Social_Network\\client\\src\\pages\\discover.js",["429"],"import React, { useState, useEffect } from 'react';\r\nimport { useSelector, useDispatch } from 'react-redux';\r\nimport { getDiscoverPosts, DISCOVER_TYPES } from '../redux/actions/discoverAction';\r\nimport LoadIcon from '../assets/images/loading.gif';\r\nimport PostThumb from '../components/PostThumb';\r\nimport LoadMoreBtn from '../components/button/LoadMoreBtn';\r\nimport { getDataAPI } from '../utils/fetchData';\r\nimport Helmet from '../components/Helmet';\r\nimport LoadData from '../components/alert/LoadData';\r\n\r\nconst Discover = () => {\r\n  const { auth, discover } = useSelector((state) => state);\r\n  const dispatch = useDispatch();\r\n\r\n  const [load, setLoad] = useState(false);\r\n\r\n  useEffect(() => {\r\n    if (!discover.firstLoad) {\r\n      dispatch(getDiscoverPosts(auth.token));\r\n    }\r\n  }, [dispatch, auth.token, discover.firstLoad]);\r\n\r\n  const handleLoadMore = async () => {\r\n    setLoad(true);\r\n    const res = await getDataAPI(`post_discover?num=${discover.page * 9}`, auth.token);\r\n    dispatch({ type: DISCOVER_TYPES.UPDATE_POST, payload: res.data });\r\n    setLoad(false);\r\n  };\r\n\r\n  return (\r\n    <Helmet title=\"Khám phá\">\r\n      <div className=\"main\">\r\n        {discover.loading ? <LoadData /> : <PostThumb posts={discover.posts} result={discover.result} />}\r\n\r\n        {load && <LoadData />}\r\n\r\n        {!discover.loading && (\r\n          <LoadMoreBtn result={discover.result} page={discover.page} load={load} handleLoadMore={handleLoadMore} />\r\n        )}\r\n      </div>\r\n    </Helmet>\r\n  );\r\n};\r\n\r\nexport default Discover;\r\n","D:\\UTC2_Social_Network\\client\\src\\pages\\forgotPassword.js",["430"],"import React, { useState