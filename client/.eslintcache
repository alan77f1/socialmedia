mail của bạn.\";\r\n  } else if (!validateEmail(email)) {\r\n    err.email = \"Định dạng email không đúng.\";\r\n  }\r\n\r\n  if (!password) {\r\n    err.password = \"Vui lòng thêm mật khẩu của bạn.\";\r\n  } else if (password.length < 6) {\r\n    err.password = \"Mật khẩu phải từ 6 kí tự trở lên.\";\r\n  }\r\n\r\n  if (password !== cf_password) {\r\n    err.cf_password = \"Mật khẩu không trùng khớp.\";\r\n  }\r\n\r\n  return {\r\n    errMsg: err,\r\n    errLength: Object.keys(err).length,\r\n  };\r\n};\r\n\r\nfunction validateEmail(email) {\r\n  // eslint-disable-next-line\r\n  const re =\r\n    /^(([^<>()[\\]\\\\.,;:\\s@\\\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\\\"]+)*)|(\\\".+\\\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/;\r\n  return re.test(email);\r\n}\r\n\r\nexport default valid;\r\n","D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\alertReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\authReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\themeReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\statusReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\profileReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\modalReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\postReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\detailPostReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\discoverReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\socketReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\suggestionsReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\notifyReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\onlineReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\peerReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\redux\\reducers\\callReducer.js",[],"D:\\TT\\utc2-network\\client\\src\\r